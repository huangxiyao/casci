  <builders>
    <hudson.tasks.Shell>
      <command>[[ -z &quot;${COMPANY}&quot; ]] &amp;&amp; echo &quot;COMPANY must be specified&quot; &amp;&amp; exit 1
[[ -z &quot;${ENVIRONMENT}&quot; ]] &amp;&amp; echo &quot;ENVIRONMENT must be specified&quot; &amp;&amp; exit 1
[[ -z &quot;${PLAYBOOK_VERSION}&quot; ]] &amp;&amp; echo &quot;PLAYBOOK_VERSION must be specified&quot; &amp;&amp; exit 1

[[ &quot;${PLAYBOOK_VERSION}&quot; == &quot;latest&quot; ]] &amp;&amp; PLAYBOOK_VERSION=$(pipeline.sh lastStablePlaybookVersion)
[[ -z &quot;${PLAYBOOK_VERSION}&quot; ]] &amp;&amp; echo &quot;There is no stable playbook build&quot; &amp;&amp; exit 1

if [[ -z &quot;${UNIX_USERNAME}&quot; ]]
then
	UNIX_USERNAME=$(pipeline.sh unixUserName &quot;${BUILD_USER_ID}&quot;)
	[[ -z &quot;${UNIX_USERNAME}&quot; ]] &amp;&amp; echo &quot;UNIX_USERNAME must be specified. Are you logged into Jenkins?&quot; &amp;&amp; exit 1
fi

if [[ &quot;${UNIX_USERNAME}&quot; != &quot;casfw&quot; ]]
then
	[[ -z &quot;${UNIX_PASSWORD}&quot; ]] &amp;&amp; echo &quot;UNIX_PASSWORD must be specified&quot; &amp;&amp; exit 1
	export SSHPASS=&quot;${UNIX_PASSWORD}&quot;
fi

if [[ &quot;${ENVIRONMENT}&quot; == &quot;dev&quot; ]]
then
    [[ -z &quot;${PLATFORM_VERSION}&quot; ]] &amp;&amp; echo &quot;PLATFORM_VERSION must be specified&quot; &amp;&amp; exit 1
	[[ &quot;${PLATFORM_VERSION}&quot; == &quot;latest&quot; ]] &amp;&amp; PLATFORM_VERSION=$(pipeline.sh lastStablePlatformVersion)
	[[ -z &quot;${PLATFORM_VERSION}&quot; ]] &amp;&amp; echo &quot;There is no stable platform build&quot; &amp;&amp; exit 1
fi

git checkout refs/tags/${PLAYBOOK_VERSION}

if [[ &quot;${ENVIRONMENT}&quot; == &quot;dev&quot; ]]
then
	pipeline.sh editInventory &quot;${PLATFORM_VERSION}&quot; inventory/${COMPANY}/${ENVIRONMENT}/group_vars/all
else
    pipeline.sh archiveToNexus &quot;${COMPANY}&quot; &quot;${ENVIRONMENT}&quot;
fi

export VAULT_PASSWORD_FILE
 
echo &quot;Deploying to ${COMPANY}/${ENVIRONMENT} using playbook ${PLAYBOOK_VERSION}.&quot;
failed=1
deploy.sh &quot;${BUILD_ID}&quot; &quot;${DEPLOY_VERSION}&quot; &quot;${COMPANY}&quot; &quot;${ENVIRONMENT}&quot; &quot;${PLAYBOOK_VERSION}&quot; &quot;${UNIX_USERNAME}&quot; &amp;&amp; failed=0
report.sh
notify.sh &quot;${BUILD_ID}&quot;
 
exit ${failed}
</command>
    </hudson.tasks.Shell>
